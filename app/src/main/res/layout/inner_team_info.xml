            android:id="@+id/tv_open_project_plan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:background="@drawable/rectangle_fill_1c2e52_radius_10"
            android:fontFamily="@font/pretendard_regular"
            android:includeFontPadding="false"
            android:paddingHorizontal="10dp"
            android:paddingVertical="2dp"
            android:text="@string/open_project_plan"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/img_person1"
            app:layout_constraintStart_toStartOf="@+id/tv_open_project_plan"
            app:layout_constraintTop_toBottomOf="@+id/tv_open_project_plan" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="이정훈"
            android:textColor="@color/black"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/imageView" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="18dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/img_yoon"
            app:layout_constraintStart_toEndOf="@+id/textView16"
            app:layout_constraintTop_toBottomOf="@+id/tv_open_project_plan" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="정윤정"
            android:textColor="@color/black"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView1"
            app:layout_constraintStart_toEndOf="@+id/imageView1"
            app:layout_constraintTop_toTopOf="@+id/imageView1" />


        <TextView
            android:id="@+id/tv_open_project_develop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:background="@drawable/rectangle_fill_1c2e52_radius_10"
            android:fontFamily="@font/pretendard_regular"
            android:includeFontPadding="false"
            android:paddingHorizontal="10dp"
            android:paddingVertical="2dp"
            android:text="@string/open_project_develop"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />


        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/img_kite"
            app:layout_constraintStart_toStartOf="@+id/tv_open_project_develop"
            app:layout_constraintTop_toBottomOf="@+id/tv_open_project_develop" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
